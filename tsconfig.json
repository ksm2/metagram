{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "declaration": true,
    "noEmitOnError": true,
    "pretty": true,
    "lib": ["es2015", "es2015.collection", "es2015.promise", "dom"],
    "strictNullChecks": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "typeRoots": ["node_modules/@types", "custom_typings"]
  }
}
