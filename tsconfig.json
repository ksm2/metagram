{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "declaration": true,
    "noEmitOnError": true,
    "pretty": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dest"
  },
  "files": [
    "./custom_typings/canvas.d.ts",
    "./src/server.ts",
    "./src/client.ts"
  ]
}
